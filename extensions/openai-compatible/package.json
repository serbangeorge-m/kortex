{
  "name": "openai-compatible",
  "displayName": "OpenAI Compatible",
  "description": "Integration for Open AI",
  "version": "0.0.1-next",
  "icon": "icon.png",
  "publisher": "kortex",
  "license": "Apache-2.0",
  "engines": {
    "kortex": "^0.0.1"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "OpenAI",
      "properties": {
        "openai.factory.baseURL": {
          "type": "string",
          "scope": "InferenceProviderConnectionFactory",
          "description": "baseURL"
        },
        "openai.factory.apiKey": {
          "type": "string",
          "scope": "InferenceProviderConnectionFactory",
          "description": "apiKey"
        }
      }
    }
  },
  "scripts": {
    "build": "vite build && node ./scripts/build.js",
    "test": "vitest run --coverage --passWithNoTests",
    "test:watch": "vitest watch --coverage --passWithNoTests",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^2.0.2",
    "@kortex-app/api": "workspace:*"
  },
  "devDependencies": {
    "mkdirp": "^3.0.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
